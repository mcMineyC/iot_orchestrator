{
  "host": "0.0.0.0",
  "port": 1883,
  "enabledIntegrations": [
    {
      "name": "My Simple Greeter",
      "integrationName": "greeter",
      "id": "test-greeter",
      "config": {
        "name": "John"
      }
    }
  ],
  "knownIntegrations": {
    "greeter": {
      "name": "Greeter",
      "manage": true,
      "command": "node greeter.js",
      "args": ["name"],
      "schema": [
        {
          "path": "/name",
          "type": "data",
          "fetchable": true
        },
        {
          "path": "/names",
          "type": "data",
          "fetchable": true
        },
        {
          "path": "/greeting",
          "type": "data",
          "fetchable": false
        },
        {
          "path": "/greet",
          "type": "command",
          "fetchable": false
        },
        {
          "path": "/hello",
          "type": "command",
          "fetchable": false
        }
      ]
    },
    "kasa": {
      "name": "TP-Link Kasa",
      "manage": true,
      "command": "node kas.js",
      "args": ["ip", "type"],
      "schema": [
        {
          "path": "/powerState",
          "type": "data",
          "fetchable": true
        },
        {
          "path": "/lightState",
          "type": "data",
          "fetchable": true
        },
        {
          "path": "/power/on",
          "type": "command",
          "fetchable": false
        },
        {
          "path": "/power/off",
          "type": "command",
          "fetchable": false
        },
        {
          "path": "/light/brightness",
          "type": "command",
          "fetchable": false
        },
        {
          "path": "/light/temperature",
          "type": "command",
          "fetchable": false
        },
        {
          "path": "/light/color",
          "type": "command",
          "fetchable": false
        },
        {
          "path": "/light/default",
          "type": "command",
          "fetchable": false
        }
      ]
    }
  }
}
